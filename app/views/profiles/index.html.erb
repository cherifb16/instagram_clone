<p id="notice"><%= notice %></p>
      <div style="margin-left:250px;">
      <% if user_signed_in? %>
      <% check = 0, links='1', id=nil %>
      <% @profiles.each do |profile| %>
      <% if current_user.id == profile.user_id %>
      <% check = 1%>
      <%links = profile.image.url %>
      <%id =profile%>
      <% end %>
      <% end %>
      <% if check==1 %>
      <h4>profile picture</h4>
      <%= image_tag links, class:"brand_logo" unless links.blank? %>
      <strong class="user">
      <%= link_to 'change Profile Picture', edit_profile_path(id), class:"btn btn-primary" %>
      <%= link_to 'remove Profile Picture', id, method: :delete, data: { confirm: 'Are you sure?' },class:"btn btn-danger" %>
      </br></br>

      <%= link_to 'Posts', posts_path, class:"btn btn-dark" %>
      <%= image_tag "cherif1.png" %>
      <% else %>
      <%= image_tag "instagram45.png", class:"brand_logo" %>
  <%= link_to 'upload a profile picture', new_profile_path, class:"btn btn-success" %>
  <%= link_to 'Posts', posts_path, class:"btn btn" %>
  <% end %>
  <% end %>
  </strong>
</div>